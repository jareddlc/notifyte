{
  "name": "notifyte",
  "version": "1.0.0",
  "description": "Smart phone notifications via bluetooth",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "./node_modules/.bin/electron .",
    "rebuild": "./node_modules/.bin/electron-rebuild",
    "build-all": "./node_modules/.bin/electron-packager ./ --overwrite --app-version=$npm_package_version --name=Notifyte --all --out=./build --version=1.2.2",
    "build-osx": "./node_modules/.bin/electron-packager ./ --overwrite --app-version=$npm_package_version --name=Notifyte --platform=darwin --arch=x64 --out ./build --version 1.2.2 --overwrite",
    "build-win": "./node_modules/.bin/electron-packager ./ --overwrite --app-version=$npm_package_version --name=Notifyte --platform=win32 --arch=ia32 --out ./build --version 1.2.2 --overwrite",
    "build-lin": "./node_modules/.bin/electron-packager ./ --overwrite --app-version=$npm_package_version --name=Notifyte --platform=linux --arch=x64 --out=./build --version=1.2.2 --overwrite",
    "bower-install": "node_modules/.bin/bower install",
    "gulp-bower": "node_modules/.bin/gulp bower",
    "gulp": "node_modules/.bin/gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jareddlc/notifyte.git"
  },
  "author": "Jared De La Cruz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jareddlc/notifyte/issues"
  },
  "homepage": "https://github.com/jareddlc/notifyte#readme",
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "bleno": "^0.4.0",
    "body-parser": "^1.15.1",
    "cookie-parser": "^1.4.1",
    "electron-prebuilt": "^1.2.2",
    "electron-rebuild": "^1.1.5",
    "express": "^4.13.4",
    "hbs": "^4.0.0",
    "lodash": "^4.13.1",
    "memory-cache": "^0.1.5",
    "socket.io": "^1.4.6"
  },
  "devDependencies": {
    "bower": "^1.7.7",
    "electron-packager": "^7.0.3",
    "gulp": "^3.9.1"
  }
}
